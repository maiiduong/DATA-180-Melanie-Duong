library(MASS)
data(Cars93)
head(Cars93)
# Custom options for knitting
knitr::opts_chunk$set(
message = FALSE,
warning = FALSE,
error = FALSE,
collapse = TRUE,
comment = "#",
fig.height = 5,
fig.width = 5,
fig.align = "center",
cache = FALSE
